/*select*/
.selectbox {
	width: 150px;
	position: relative;
	float: left;
}
.tit {
	display: block;
	height: 26px;
	line-height: 26px;
	padding: 0 15px;
	background: linear-gradient(#fafafa, #f4f4f4 50%, #e5e5e5);
	cursor: pointer;
	border-radius: 5px 5px 0 0;
	border: 1px solid #ccc;
	position: relative;
}
body .select_ul {
	width: 148px;
	border: 1px solid #ccc;
	border-top: none;
	display: none;
	border-radius: 0 0 5px 5px;
	position: absolute;
	z-index: 99;
	background: #fff;
	
}
body .select_ul li {
	height: 28px;
	line-height: 28px;
	display: block;
	padding: 0 15px;
	cursor: pointer;
	margin:0;
	background:#fff;
}
.select_ul li.hover, .select_ul  li:hover {
	background: #e5e5e5;
	color: #333;
}
.select_ui {
	display: none;
}
.ico {
	width: 0;
	height: 0;
	border: 6px solid #000;
	border-color: #333 transparent transparent transparent;
	position: absolute;
	top: 10px;
	right: 10px;
}
.title_hover .ico {
	border-color: transparent transparent #333 transparent;
	top: 4px;
}
/*radio*/
.radio_box {
	width: 230px;
	height: 25px;
	line-height: 25px;
	display: block;
	float: left;
*display:inline;
}
.radio_box b {
	float: left;
*display:inline;
	width: 20px;
	height: 25px;
	background: url(../bg/uibg.png) no-repeat -280px -255px;
	cursor:pointer;
}
.radio_box label {
	line-height: 25px;
	height: 25px;
	float: left;
}
.radio_box_hover {
	display: block;
}
.radio_box_hover b {
	background: url(../bg/uibg.png) no-repeat -260px -255px;
}
/*checked*/
.autoSwitch{cursor:pointer;vertical-align: middle;width:160px; height:40px;background: url(../bg/button.png) no-repeat 0px -40px;}
.autoSwitch_hover{background-position:  0px 0px;}
.check_box{cursor:pointer;vertical-align: middle;display: inline-block;width:30px; height:18px;background: url(../bg/button.png) no-repeat 0px -98px;}
.check_box_hover{background-position:0px -80px;}
.radioBox{display: inline-block;cursor:pointer;padding: 0 5px 0 0;}
.radioBox label{display: inline-block;cursor:pointer;padding: 0 0 0 5px;}
.radioBox b{display: inline-block;width:18px; height:18px;vertical-align: middle;background: url(../bg/button.png) no-repeat 0px -116px; }
.radioBox_hover b{background-position:0px -134px;}

